                "Russia",
                "Spain",
                "Thailand",
                "Vietnam",
                "United Kingdom",
                "United States of America"
            ];
            $("#country").autocomplete({
                source: availableTags
            });
        });
        document.querySelectorAll(".reset").forEach(btn => {
            btn.onclick = function () {
                const type = this.dataset.reset;

                if (type === "date") {
                    document.querySelectorAll("#dateFilter .dropdown-toggle")
                        .forEach(b => b.textContent = b.dataset.default);
                }
            };
        });

        $('.slider').slick({
            infinite: true,
            dots: false,
            slidesToShow: 1,
            slidesToScroll: 1,
            nextArrow: '<i class="fa-solid fa-angle-right"></i></a>',
            prevArrow: '<i class="fa-solid fa-angle-left"></i></a>'
        });
        $(document).ready(function () {
            $(".bars").click(function () {
                $(".mobile-bottom").slideToggle(1500);
            });
        });

        $("#filter").on("click", function (e) {
            e.preventDefault();
            $(".more-filters").slideToggle(1000);
        });

        $(".dropdown-item").click(function () {
            $(this).closest(".dropdown")
                .find(".dropdown-toggle")
                .text($(this).text());
        });

        const range = document.getElementById("range");
        function updateFill() {
            let percent = ((range.value - range.min) / (range.max - range.min)) * 100;
            range.style.setProperty("--fill", percent + "%");
        }
        updateFill();
        range.addEventListener("input", updateFill);
    </script>
</body>

</html>
